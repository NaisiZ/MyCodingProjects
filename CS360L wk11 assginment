#include <iostream>
#include <string>
using namespace std;

class Department
{
private:
    int deptno;
    string dname;
public:
    void setDeptno(int Deptno)
    {
        deptno=Deptno;
    }
    int getDeptno()
    {
        return deptno;
    }
    void setDname(string Dname)
    {
        dname=Dname;
    }
    string getDname()
    {
        return dname;
    }
};

MyClass::deptno(int x);
MyClass::dname(string y);

class Employee: Department
{
private:
    int empno;
    string ename;
public:
    void setEmpno(int Empno)
    {
        empno=Empno;
    }
    int getEmpno()
    {
        return empno;
    }
    void setEname(string Ename)
    {
        ename=Ename;
    }
    string getEname()
    {
        return ename;
    }
};

int main() {
    
    return 0;
}
