#include <iostream>
using namespace std;

union Str
{
    int x;
    int y;
};
int main()
{
    Str s1;
    s1.x=10;
    cout<<s1.x<<endl;
    s1.y=20;
    cout<<s1.x<<endl;
    cout<<s1.y<<endl;
    s1.x=22;
    
    cout<<s1.x<<endl;
    cout<<s1.y<<endl;
    
    return 0;
}
